<UserControl
    x:Class="BasicExample.Views.PageB"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:BasicExample.Views"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    d:DesignHeight="450"
    d:DesignWidth="800"
    mc:Ignorable="d">
    <StackPanel Margin="10">
        <TextBlock FontWeight="Bold" Text="Current name:" />
        <TextBlock Text="{Binding PageBViewModel.Name, Mode=OneWay}" />
    </StackPanel>
</UserControl>
